<%= render(:partial => 'groups/menu')%>

<h3 style="clear:both">Import</h3>


<form action="/groups/<%= @group.id %>/importdo" method="post">
<%= hidden_field_tag :authenticity_token, form_authenticity_token %>
	
<p>Enter names and addresses, in this format: email,first,last, one person per line:</p>

<p><%= text_area_tag 'new_text', nil, :rows => 5, :cols => 30 %></p>

<p><input type="submit" value="Submit"></p>

</form>